@import "~@angular/material/prebuilt-themes/indigo-pink.css";
.login_page-wrapper
  width: 100%;
  min-height: 100vh;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  &:after{
    content: '';
    position absolute;
    width: 100%;
    height: 100%;
    background-color: rgba(black , 0.5)
  }
.google_registration{
  max-width: 250px;
  background-color red;
